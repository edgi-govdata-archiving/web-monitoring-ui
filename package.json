  "license": "GPL-3.0",
  "engines": {
    "node": "18.12.1"
  },
  "browserslist": [
    "last 3 versions",
    "not < 1%",
    "not ie < 1000",
    "not op_mini all",
    "not android < 1000",
    "not dead"
  ]
